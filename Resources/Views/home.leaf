#set("title") { Home }
#set("body") {
    <h1>Bob Dylan Fan Club</h1>
    #if(username) {
        <p>Welcome back, #(username)!</p>
        <ul>
        #for(forum in forums) {
            <li><a href="/forums/#(forum.id)">#(forum.name)</a></li>
        }
        </ul>
    } else {
        <a href="/users/login">Login</a>
    }
}

#embed("master")
